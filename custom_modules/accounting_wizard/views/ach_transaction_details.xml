<odoo>
<record id="ach_transaction_tree" model="ir.ui.view">
    <field name="name">ach.transaction.details.tree</field>
    <field name="model">ach.transaction.details</field>
    <field name="arch" type="xml">
        <list string="Test ACH" create="false">
            <field name="account_holder"/>
            <field name="account_number"/>
            <field name="routing_number"/>
            <field name="date"/>
            <field name="reference_ach"/>
        </list>
    </field>
</record>

<record id="ach_transaction_action" model="ir.actions.act_window">
    <field name="name">ACH Transaction Details</field>
    <field name="res_model">ach.transaction.details</field>
    <field name="view_mode">tree</field>
</record>
</odoo>