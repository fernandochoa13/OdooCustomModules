<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="land_acquisition_stage_view_tree" model="ir.ui.view">
            <field name="name">land.acquisition.stage.tree</field>
            <field name="model">land.acquisition.stage</field>
            <field name="arch" type="xml">
                <list editable="top">
                    <field name="sequence" widget="handle"/>
                    <field name="name"/>
                </list>
            </field>
        </record>
        <record id="action_land_acquisition_stage" model="ir.actions.act_window">
            <field name="name">Land Acquisition Stages</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">land.acquisition.stage</field>
            <field name="view_mode">tree</field>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>