<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="land_acquisition_tag_view_tree" model="ir.ui.view">
            <field name="name">land.acquisition.tag.tree</field>
            <field name="model">land.acquisition.tag</field>
            <field name="arch" type="xml">
                <list editable="top">
                    <field name="name"/>
                    <field name="color" widget="color_picker"/>
                </list>
            </field>
        </record>
        <record id="action_land_acquisition_tag" model="ir.actions.act_window">
            <field name="name">Land Acquisition Tags</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">land.acquisition.tag</field>
            <field name="view_mode">tree</field>
            <field name="context">{}</field>
        </record>
    </data>
</odoo>