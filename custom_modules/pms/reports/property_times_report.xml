<odoo>

<record id="property_times_report_search" model="ir.ui.view">
        <field name="name">property.times.report.search</field>
        <field name="model">property.times.report</field>
        <field name="arch" type="xml">
            <search string="Default Search">
                <field name="property_name"/>
                <field name="house_model"/>
                <field name="construction_phase"/>
                <field name="engineering_fee_duration"/>
                <field name="survey_issued_duration"/>
                <field name="plot_plan_issued_duration"/>
                <field name="construction_draw_calc_duration"/>
                <field name="energy_calcs_duration"/>
                <field name="septic_permit_issue_duration"/>
                <field name="truss_calcs_duration"/>
                <field name="permit_pip_duration"/>
                <field name="permit_applied_duration"/>
                <field name="permit_issued_duration"/>
                <field name="total_days"/>
            </search>
        </field>
</record>

<record id="property_times_report_tree" model="ir.ui.view">
    <field name="name">property.times.report.tree</field>
    <field name="model">property.times.report</field>
    <field name="arch" type="xml">
        <list string="Property Times Report" create="false">
            <field name="property_name"/>
            <field name="house_model"/>
            <field name="construction_phase"/>
            <field name="engineering_fee_duration"/>
            <field name="survey_issued_duration"/>
            <field name="plot_plan_issued_duration"/>
            <field name="construction_draw_calc_duration"/>
            <field name="energy_calcs_duration"/>
            <field name="septic_permit_issue_duration"/>
            <field name="truss_calcs_duration"/>
            <field name="permit_pip_duration"/>
            <field name="permit_applied_duration"/>
            <field name="permit_issued_duration"/>
            <field name="total_days"/>
        </list>
    </field>
</record>


<record id="property_times_report_report_action" model="ir.actions.act_window">
    <field name="name">Property Times Report</field>
    <field name="res_model">property.times.report</field>
    <field name="view_mode">tree</field>
    <field name="search_view_id" ref="pms.property_times_report_search"/>
</record>

</odoo>