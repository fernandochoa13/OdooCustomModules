<odoo>
    <record id="view_dashboard_config_tree" model="ir.ui.view">
        <field name="name">dashboard.config.tree</field>
        <field name="model">dashboard.config</field>
        <field name="arch" type="xml">
            <list string="Dashboard Config">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="view_dashboard_config_forn" model="ir.ui.view">
        <field name="name">dashboard.config.form</field>
        <field name="model">dashboard.config</field>
        <field name="arch" type="xml">
            <form string="Dashboard Config">
                <sheet>
                <div class="oe_title">
                    <h1>
                        <field name="name" widget="char" class="o_bold_title"/>
                    </h1>
                </div>
                    <group>
                        <field name="url"/>
                        <field name="secret_key" password="true"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_dashboard_config" model="ir.actions.act_window">
        <field name="name">Dashboard Config</field>
        <field name="res_model">dashboard.config</field>
        <field name="view_mode">list,form</field>
    </record>


    <record model="ir.actions.act_url" id="action_google_url">
        <field name='name'>Activity Report</field>
        <field name='url'>https://adanordonezp.com/dashboard/activitiesreport?token=123
        <field name='target'>new</field>
    </record>

</odoo>
