<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="pms_provider_config_view_tree" model="ir.ui.view">
    <field name="name">pms.provider.config.tree</field>
    <field name="model">pms.provider.config</field>
    <field name="arch" type="xml">
        <list string="Projects Provider Config" editable="top">
            <field name="partner_id" optional="show"/>
            <field name="company_id" optional="show"/>
        </list>
    </field>
</record>

<record id="pms_provider_config_view" model="ir.actions.act_window">
    <field name="name">Projects Provider Config</field>
    <field name="res_model">pms.provider.config</field>
    <field name="view_mode">list</field>
</record>

</odoo>

 