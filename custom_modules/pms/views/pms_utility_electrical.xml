<odoo>

    <record id="pms_properties_utility_electrical_search_view" model="ir.ui.view">
                <field name="name">pms.utility.search</field>
                <field name="model">pms.property</field>
                <field name="arch" type="xml">
                    <search string="Utility Electrical Search">
                        <field name="name"/>
                        <field name="county"/>
                        <field name="parcel_id"/>
                        <field name="on_hold"/>
                        <separator/>
                        <filter string="On Hold" name="filteronhold" domain="[('on_hold', '=', True)]"/>
                    </search>
                </field>
    </record>

<record id="pms_properties_utility_electrical_view_tree" model="ir.ui.view">
    <field name="name">pms.utility.tree</field>
    <field name="model">pms.property</field>
    <field name="arch" type="xml">
        <list string="Utility Electrical Table" editable="bottom">
                                                        <!-- General -->
            <field name="name" optional="show" readonly="1"/>
            <field name="county" optional="show" readonly="1"/>
            <field name="parcel_id" optional="show" readonly="1"/>
            <field name="house_model" optional="show" readonly="1"/>
            <field name="partner_id" optional="show" readonly="1"/>
            <field name="partner_tax_id" optional="show" readonly="1"/>
            <field name="utility_phase" optional="show" readonly="1"/>
            <field name="superintendent" optional="show" readonly="1"/>
            <field name="on_hold" optional="show" readonly="1"/>
            <field name="general_sunshine" optional="show"/> 
                                                        <!-- Electrical -->
            <field name="e_type" optional="show"/>
            <field name="e_work_order" optional="show"/>
            <field name="e_account_number" optional="show"/>
            <field name="e_app_date" optional="show"/>
            <field name="e_conn_type" optional="show"/>
            <field name="e_invoice" optional="show"/>
            <field name="e_payment_date" optional="show"/>
            <field name="elect_meter" optional="show"/>
            <field name="e_utility_provider" optional="show"/>
            <field name="elect_meter_position" optional="show"/>
            <field name="elect_meter_request" optional="show"/>
            <field name="e_conn_status" optional="show"/>
            <field name="e_conn_date" optional="show"/>
            <field name="e_comments" optional="show"/>
            <field name="elect_meter_request_date" optional="show"/>
            <field name="e_disconn_date" optional="show"/>
            <field name="elect_utility_engineer" optional="show"/>
            <field name="e_tug" optional="show"/>
            <field name="e_monthly_bill" optional="show"/>
            <field name="e_monthly_payment_date" optional="show"/>
            <field name="e_payment_request" optional="show"/>

        </list>
    </field>
</record>

<record id="pms_properties_utility_electrical_view" model="ir.actions.act_window">
    <field name="name">Electrical Utilities</field>
    <field name="res_model">pms.property</field>
    <field name="view_mode">list</field>
    <field name="view_id" ref="pms_properties_utility_electrical_view_tree"/>
    <field name="context">{'tree_view_ref' : 'pms_properties_utility_electrical_view_tree'}</field>
    <field name="search_view_id" ref="pms_properties_utility_electrical_search_view"/>
</record>

<record id="action_server_create_electric" model="ir.actions.server">
    <field name="name">Create Electric Payment Request</field>
    <field name="type">ir.actions.server</field>
    <field name="model_id" ref="pms.model_pms_property"/>
    <field name="binding_model_id" ref="pms.model_pms_property"/>
    <field name="state">code</field>
    <field name="code">
    if records:
    records.wrapper_electric_payment_request()
    </field>
</record>


</odoo>

